<h1>Posts#index</h1>
<h1>Listing Posts</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 <% @posts.each do |p| %>
    <tr>
      <td><%= p.title %></td>
      <td><%= p.body %></td>
      <td><%= link_to 'Show', post_path(p) %> </td>
      <td><%= link_to 'Edit', edit_post_path(p) %> </td>
    </tr>
  <% end %>
  
</table>

<h2> <%= link_to 'New', new_post_path %> </h2>
