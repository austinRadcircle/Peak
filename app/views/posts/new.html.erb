<h1>Posts#new</h1>

<%= form_for :post, url: posts_path do |f| %>
  <p>
    <%= f.label :form %>
    <select>
    <option value= ""> Choose Form </option>
    <option value="Quick" >Quick </option>
    <option value="Review" >Review</option>
    <option value="Long">Long</option>
    </select>
    
  </p>
  
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
 
    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </p>
 
    <p>
      <%= f.submit %>
    </p>

  
<% end %>
<h2> <%= link_to 'Home', posts_path %> </h2>
