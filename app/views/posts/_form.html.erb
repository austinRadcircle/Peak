<%= form_for @post do |f| %>
  
  
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :subtitle %><br>
      <%= f.text_field :subtitle %>
    </p>
 
    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </p>

    <p>
      <%= f.label :link %><br>
      <%= f.text_field :link %>
    </p>
 
    <p>
      <%= f.label :artist %><br>
      <%= f.text_field :artist %>
    </p>

   <p>
      <%= f.label :song %><br>
      <%= f.text_field :song %>
    </p>

    <p>
      <%= f.submit %>
    </p>

  
<% end %>