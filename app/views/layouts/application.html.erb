<!DOCTYPE html>
<html>
<head>
  <title>Peak</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<section id = "main">
<% [:notice, :alert].each do |level| %>
	<% if flash[level] %>
		<div class = "flash <%= level%>"><%= flash[level] %> </div>
	<%end%>
<%end%>
<%= yield %>
<%= user_logged_in_msg %>
</section>
</body>
</html>
